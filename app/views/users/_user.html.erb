<div id="<%= dom_id user %>">
  <p>
    <strong>Username:</strong>
    <%= user.username %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Password hash:</strong>
    <%= user.password_hash %>
  </p>

  <p>
    <strong>Profile icon:</strong>
    <%= user.profile_icon %>
  </p>

  <p>
    <strong>Coment count:</strong>
    <%= user.coment_count %>
  </p>

  <p>
    <strong>Report count:</strong>
    <%= user.report_count %>
  </p>

</div>
